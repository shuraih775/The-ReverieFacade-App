{
  "expo": {
    "name": "reveriefacadeapprebuild",
    "slug": "reveriefacadeapprebuild",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "com.reveriefacade",
    
    "assetBundlePatterns": ["assets/*"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reveriefacade"
    },
    "android": {
      "package": "com.reveriefacade",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode":1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86ba858a-0330-463a-8575-11f381c9889c"
      }
    },
    "updates": {
    "enabled": true
  },
    "owner": "shuraih"
  }
}
